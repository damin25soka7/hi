# SearXNG MCP Server Configuration
# Copy this file to .env and customize the values

# SearXNG instance URL
SEARXNG_BASE_URL=http://localhost:32768

# Server host (use 127.0.0.1 for local only, 0.0.0.0 for network access)
HOST=127.0.0.1

# Server port
PORT=32769

# Timezone for datetime operations (e.g., Asia/Seoul, America/New_York, UTC)
DESIRED_TIMEZONE=UTC

# Maximum content length per page
CONTENT_MAX_LENGTH=10000

# Default search result limit
SEARCH_RESULT_LIMIT=10
